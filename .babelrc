{
  "presets": [
    "stage-2",
    ["env", {
      "targets": {
        "electron": "1.7",
        "node": "7.9.0"
      }
    }],
    "react"
  ],
  "plugins": [
    "inline-react-svg",
    [
      "react-css-modules",
      {
        "generateScopedName": "[local]_[hash:base64:5]",
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss"
          },
          ".sass": {
            "syntax": "postcss-sass"
          }
        }
      }
    ]
  ]
}
