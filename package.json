	"scripts": {
		"start": "run-p 'watch' 'styleguide' 'create-entrypoints'",
		"styleguide": "styleguidist server",
		"clean-dist": "shx rm -rf dist && shx mkdir dist",
		"build": "yarn run clean-dist && yarn run create-ts-svgs && yarn build:webpack && yarn build:sass && yarn run create-entrypoints",
		"build:webpack": "webpack --config webpack.prod.js",
		"build:sass": "node-sass src/global.sass | postcss --map -o dist/global.css",
		"watch": "yarn run clean-dist && yarn run create-ts-svgs && run-p 'build:sass' 'watch:webpack' 'watch:sass'",
		"watch:webpack": "webpack-dev-server --config webpack.dev.js",
		"watch:sass": "chokidar 'src/**/*.sass' 'src/**/*.scss' -c 'yarn build:sass'",
		"test": "jest",
		"prepare": "yarn run build && shx rm dist/global.css.json",
		"create-entrypoints": "ts-node --transpileOnly tasks/create-component-entrypoints.ts",
		"create-ts-svgs": "ts-node tasks/create-typescript-svgs.ts",
		"gh-pages:build": "styleguidist build",
		"gh-pages:deploy": "styleguidist build && gh-pages -d styleguide",
		"tslint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json -t stylish"
	},
	"peerDependencies": {
		"react": ">= 16.4.0",
		"react-dom": ">= 16.4.0",
