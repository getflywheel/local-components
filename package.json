  "scripts": {
    "start": "run-p 'watch' 'styleguide' 'create-entrypoints'",
    "styleguide": "yarn storybook",
    "clean-dist": "shx rm -rf dist && shx mkdir dist",
    "build": "yarn run clean-dist && yarn build:webpack && yarn build:sass && yarn run create-entrypoints",
    "build:webpack": "webpack --config webpack.prod.js --progress",
    "build:sass": "node-sass src/global.sass | postcss --map -o dist/global.css",
    "watch": "yarn run clean-dist && run-p 'build:sass' 'watch:webpack' 'watch:sass'",
    "watch:webpack": "webpack-dev-server --config webpack.dev.js",
    "watch:sass": "chokidar 'src/**/*.sass' 'src/**/*.scss' -c 'yarn build:sass'",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "prepublishOnly": "yarn run build && shx rm dist/global.css.json",
    "create-entrypoints": "ts-node --transpile-only tasks/create-component-entrypoints.ts",
    "gh-pages:build": "yarn build-storybook",
    "gh-pages:deploy": "yarn gh-pages:build && gh-pages -d storybook-static",
    "tslint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json -t stylish",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
